

indentation:=\r\t\t\t\t

help:
	@echo -e "targets:"
	@echo -e "  help$(indentation)this output"
	@echo -e "  all$(indentation)make all outputs"
	@echo
	@echo -e "  doc$(indentation)make doc outputs"
	@echo -e "  gsoap$(indentation)generate with gsoap..."
	@echo
	@echo -e "  publish$(indentation)publish all outputs to http://filebin.net/ggyreqqek0"
	@echo
	@echo -e "  clean$(indentation)cleanup"
	@echo -e "  realclean$(indentation)cleanup more"


all:
	

doc:
	

publish:


